{
  "name": "Cardwallet",
  "private": true,
  "scripts": {
    "setup": "yarn install && yarn postinstall",
    "check-env": "./scripts/check-env.sh",
    "build:ios": "node node_modules/react-native/local-cli/cli.js bundle --entry-file='index.js' --bundle-output='./ios/main.jsbundle' --dev=false --platform='ios'",
    "android": "react-native run-android",
    "android:uninstall": "(adb uninstall com.cardstack.cardpay || true)",
    "android:clean": "yarn android:uninstall && yarn android",
    "android:apk": "yarn check-env && cd android && bash -c 'set -a && source ../.env && set +a && ./gradlew assembleRelease && yarn android:uninstall && adb install app/build/outputs/apk/release/app-release.apk'",
    "android:bundle": "yarn check-env && cd android && bash -c 'set -a && source ../.env && set +a && ./gradlew bundleRelease && open app/build/outputs/bundle/release/'",
    "android:publish:internal": "yarn check-env && cd android && bash -c 'set -a && source ../.env && set +a && yarn fastlane internal'",
    "fastlane": "bundle exec fastlane",
    "contexts:app:sync": "yarn fastlane sync_app_vars",
    "contexts:sync:google_services": "yarn fastlane sync_google_service_info",
    "contexts:sync": "yarn fastlane contexts_sync_all",
    "fast:m1": "yarn setup && yarn install-pods:m1",
    "install-bundle": "cd ios && bundle install && cd ..",
    "install-pods": "cd ios && bundle exec pod install --repo-update && cd ..",
    "install-pods:m1": "cd ios && pod install && cd ..",
    "install-pods-fast": "cd ios && bundle exec pod install && cd ..",
    "install-pods-fast-no-flipper": "cd ios && bundle exec env SKIP_FLIPPER=true pod install && cd ..",
    "install-pods-no-flipper": "cd ios && bundle exec env SKIP_FLIPPER=true pod install --repo-update && cd ..",
    "ios": "react-native run-ios --simulator='iPhone 11'",
    "ios:small": "react-native run-ios --simulator='iPhone 8'",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx --cache",
    "nodeify": "rn-nodeify --install --hack 'crypto,buffer,react-native-randombytes,vm,stream,http,https,os,url,net,fs,process'",
    "ts-check": "yarn tsc --skipLibCheck --noEmit",
    "prestorybook": "rnstl",
    "postinstall": "./scripts/postinstall.sh",
    "start": "yarn prestorybook && react-native start",
    "start:clean": "watchman watch-del-all && rm -rf $TMPDIR/react-* && rm -rf $TMPDIR/metro-* && rm -rf $TMPDIR/haste-map-* && react-native start -- --reset-cache",
    "test": "APP_ENV=test jest --coverage",
    "ci": "yarn lint && yarn ts-check",
    "android-reverse-ports": "adb reverse tcp:8097 tcp:8097 && adb reverse tcp:8081 tcp:8081",
    "update-token-list": "./scripts/update-token-list.sh",
    "reset:cache": "yarn start --reset-cache",
    "rebuild": "yarn setup && yarn install-bundle && yarn install-pods && yarn build:ios && yarn start",
    "rebuild:m1": "yarn setup && yarn install-bundle && yarn install-pods:m1 && yarn build:ios && yarn start:clean",
    "codegen": "graphql-codegen"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
